    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-terser": "^2.0.1",
    "minimist": "^1.2.5",
    "mkdirp": "^1.0.4",
    "rollup": "^3.21.0",
    "sass": "^1.62.1",
    "terser": "^5.7.1",
    "yargs": "^16.2.0"
