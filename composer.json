{
    "name": "nswdpc/waratah",
    "description": "Silverstripe integration for the NSW Design System",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "nsw",
        "module",
        "theme",
        "nsw design system"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "James Ellis",
            "role": "Developer"
        },
        {
            "name": "Mark Taylor",
            "role": "Bon Vivant"
        }
    ],
    "autoload": {
        "psr-4": {
            "NSWDPC\\Waratah\\Extensions\\": [
                "src/Extensions/"
            ],
            "NSWDPC\\Waratah\\Pages\\": [
                "src/Pages/"
            ],
            "NSWDPC\\Waratah\\Controllers\\": [
                "src/Controllers/"
            ],
            "NSWDPC\\Waratah\\Services\\": [
                "src/Services/"
            ],
            "NSWDPC\\Waratah\\Models\\": [
                "src/Models/"
            ],
            "NSWDPC\\Waratah\\Forms\\": [
                "src/Forms/"
            ]
        }
    },
    "replace": {
        "nswdpc/silverstripe-nsw-design-system": "self.version"
    },
    "require": {
        "nswdpc/silverstripe-content-boilerplate": "^1",
        "nswdpc/silverstripe-feedback-assist": "^1",
        "nswdpc/silverstripe-field-hint": "^1",
        "nswdpc/silverstripe-grid-helpers": "^1",
        "nswdpc/silverstripe-datetime-inputs": "^1",
        "nswdpc/silverstripe-notices": "^1",
        "nswdpc/silverstripe-analytics-chooser": "^1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3"
    },
    "extra": {
        "expose": [
            "themes/nswds/app/static",
            "themes/nswds/app/frontend/dist"
        ]
    }
}
